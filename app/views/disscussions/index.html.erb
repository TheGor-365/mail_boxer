<p id="notice"><%= notice %></p>

<h1>Disscussions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disscussions.each do |disscussion| %>
      <tr>
        <td><%= disscussion.title %></td>
        <td><%= link_to 'Show', disscussion %></td>
        <td><%= link_to 'Edit', edit_disscussion_path(disscussion) %></td>
        <td><%= link_to 'Destroy', disscussion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Disscussion', new_disscussion_path %>
